{
  "name": "node-express",
  "displayName": "Node.js Express",
  "version": "4.x",
  "description": "Express.js framework for Node.js APIs and web applications",
  "languages": ["typescript", "javascript"],
  "frameworks": ["express"],
  "packageManager": "npm",
  "standards": [
    "standards/express.md",
    "standards/typescript.md"
  ],
  "skills": [
    "skills/api-endpoint/SKILL.md",
    "skills/middleware/SKILL.md",
    "skills/database/SKILL.md"
  ],
  "commands": {
    "install": "npm install",
    "dev": "npm run dev",
    "build": "npm run build",
    "start": "npm start",
    "test": "npm test",
    "lint": "npm run lint"
  },
  "structure": {
    "controllers": "src/controllers",
    "routes": "src/routes",
    "services": "src/services",
    "repositories": "src/repositories",
    "middleware": "src/middleware",
    "models": "src/models",
    "types": "src/types",
    "utils": "src/utils",
    "tests": "tests"
  }
}
